{
  "name": "backend",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc -p tsconfig.json && tsc-alias -p tsconfig.json",
    "start": "node dist/index.js",
    "dev": "tsx watch --env-file=.env --include=./src src/index.ts",
    "openapi": "openapi-typescript https://raw.githubusercontent.com/PAW-paw-duarr/openapi/refs/heads/main/openapi.json -o ./src/lib/api/schema.d.ts",
    "check": "tsc --noEmit && biome check",
    "prepare": "husky || true",
    "test": "vitest"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.883.0",
    "@typegoose/typegoose": "^12.19.0",
    "argon2": "^0.44.0",
    "connect-mongo": "^5.1.0",
    "express": "^5.1.0",
    "express-session": "^1.18.2",
    "google-auth-library": "^10.3.0",
    "mongoose": "^8.18.0",
    "multer": "^2.0.2",
    "pino": "^9.9.0",
    "pino-http": "^10.5.0",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.2",
    "@types/express": "^5.0.3",
    "@types/express-session": "^1.18.2",
    "@types/multer": "^2.0.0",
    "@types/node": "^22.8.6",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.6",
    "mongodb-memory-server": "^10.2.1",
    "openapi-typescript": "^7.9.1",
    "tsc-alias": "^1.8.16",
    "tsx": "^4.20.5",
    "typescript": "^5.6.3",
    "vite": "^7.1.6",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  }
}
